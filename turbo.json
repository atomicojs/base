{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "dev": {
            "dependsOn": ["^dev"],
            "outputs": ["dist/**", "types/**"],
            "inputs": ["src/**/*.tsx", "src/**/*.ts"]
        },
        "build": {
            "dependsOn": ["^build"],
            "outputs": []
        }
    }
}
