{
    "name": "@atomico/component-1",
    "version": "1.0.0",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "atomico": "*"
    },
    "scripts": {
        "build:types": "tsc",
        "build:build": "library src/**/*",
        "build:exports": "exports lib/**/* types/**/* --wrappers",
        "dev:types": "tsc --watch",
        "dev:exports": "exports src/**/* types/**/* --watch",
        "build": "npm run build:types && npm run build:build && npm run build:exports",
        "prepublishOnly": "npm run build"
    },
    "exports": {
        "./element": "./types/element.js",
        ".": "./types/index.js",
        "./stories/index.stories": "./src/stories/index.stories.tsx",
        "./index": "./types/index.js"
    },
    "typesVersions": {
        "*": {
            "element": [
                "types/element.d.ts"
            ]
        }
    },
    "types": "types/index.d.ts",
    "main": "types/index.js",
    "module": "types/index.js"
}