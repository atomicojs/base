{
    "name": "@atomico/component-2",
    "version": "1.0.0",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "peerDependencies": {
        "atomico": "*"
    },
    "scripts": {
        "step:types": "tsc",
        "step:build": "library src/**/*",
        "step:exports": "exports lib/**/* types/**/* --wrappers",
        "step:watch:types": "tsc --watch",
        "step:watch:exports": "exports src/**/* types/**/* --watch",
        "build": "npm run step:types && npm run step:build && npm run step:exports",
        "prepublishOnly": "npm run build"
    },
    "types": "types/index.d.ts",
    "exports": {
        "./element": "./src/element.tsx",
        ".": "./src/index.tsx",
        "./stories/index.stories": "./src/stories/index.stories.tsx"
    },
    "typesVersions": {
        "*": {
            "element": [
                "types/element.d.ts"
            ]
        }
    }
}