{
        "name": "@atomico/iu-button",
        "version": "1.0.0",
        "type": "module",
        "publishConfig": {
                "access": "public"
        },
        "peerDependencies": {
                "atomico": "*"
        },
        "scripts": {
                "build:types": "tsc",
                "build:build": "library src/**/*",
                "build:exports": "exports lib/**/* types/**/* --wrappers",
                "dev:types": "tsc --watch",
                "dev:exports": "exports src/**/* types/**/* --watch",
                "build": "npm run step:types && npm run step:build && npm run step:exports",
                "prepublishOnly": "npm run build"
        },
        "types": "types/index.d.ts",
        "exports": {
                "./element": "./src/element.tsx",
                ".": "./src/index.tsx",
                "./stories/index.stories": "./src/stories/index.stories.tsx"
        },
        "typesVersions": {
                "*": {
                        "element": [
                                "types/element.d.ts"
                        ]
                }
        }
}