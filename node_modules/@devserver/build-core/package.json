{
    "name": "@devserver/build-core",
    "type": "module",
    "version": "0.12.3",
    "description": "build for @devserver/cli",
    "main": "./build.cjs",
    "module": "./src/build.js",
    "typings": "./build.d.ts",
    "exports": {
        ".": {
