		"@devserver/replace-import": "0.15.2"
	},
	"devDependencies": {
		"@betit/rollup-plugin-rename-extensions": "^0.1.0",
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-node-resolve": "9.0.0",
		"@uppercod/str-fragment": "1.3.3",
