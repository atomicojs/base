{
	"name": "@devserver/resolve",
	"type": "module",
	"version": "0.15.2",
	"description": "utilities for the replacement of imports in js files",
	"main": "./resolve.cjs",
	"module": "./src/resolve.js",
	"typings": "./resolve.d.ts",
